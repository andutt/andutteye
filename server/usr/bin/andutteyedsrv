sub bind_and_listen_for_connections {
if($Use_debug_level > 3) {
        print "[$date $time] Starting Andutteye server, waiting for incoming connections.\n";
}
if ( $Enable_ssl_encryption eq "yes" ) {
require IO::Socket::SSL;
	if($Use_debug_level > 2) {
        	print "[$date $time] Starting Andutteye server on port:$Server_listen_port with ssl encryption enabled.\n";
	}
	$socket = IO::Socket::SSL->new(
        		LocalPort 	=> $Server_listen_port,
        		Proto 		=> 'tcp',
        		Type 		=> SOCK_STREAM,
       		 	Reuse 		=> 1,
        		Listen		=> 5,
        		SSL_key_file	=> "$Use_ssl_server_key",
        		SSL_cert_file	=> "$Use_ssl_server_cert"
	) or die $!;
} else {
        if($Use_debug_level > 2) {
        	print "[$date $time] Starting Andutteye server on port:$Server_listen_port with tcp, ssl encryption disabled.\n";
        }
	$socket = IO::Socket::INET->new(
			LocalPort 	=> $Server_listen_port,
        		Type   		=> SOCK_STREAM,
        		Reuse  		=> 1,
        		Listen 		=> 10
	) or die "$!\n";
}
# End of subfunction
}
sub aesid_check {
my $hostvalue="$_[0]";

my ($sec,$min,$hour,$mday,$mon,$year,$wday,$yday,$isdst) = localtime(time);
my $date=sprintf("20%02d%02d%02d",$year%100,$mon+1,$mday);
my $time=sprintf("%02d:%02d:%02d",$hour,$min,$sec);


if($Use_debug_level > 4) {
        print "[$date,$time] Updating system status.\n";
}
$dbh = DBI->connect("dbi:$Use_dbi_database_extra_string$Use_database_type:$Use_database_name", $Use_database_user, $Use_database_password) or die ("ERROR Couldnt open database:$Use_database_name errormessage:$!");
        $sth = $dbh->prepare("update andutteye_systems set system_heartbeat = 'Last system transaction $date $time' where system_name = '$hostvalue'");
        $sth->execute;
	log_entry("Updating system status.","SystemStatusUpdate","$hostvalue");

undef $sth;
$dbh->disconnect or die $!;

if($Enable_forking_mode eq "yes") {
        print "[$date,$time] Closing child. Subfunction aesid_check.\n";
	exit;
}

# End of subfunction
}
sub back_resolv_connected_system {
my $hostvalue=$_[0];
my $system_name;
my $address;

my ($sec,$min,$hour,$mday,$mon,$year,$wday,$yday,$isdst) = localtime(time);
my $date=sprintf("20%02d%02d%02d",$year%100,$mon+1,$mday);
my $time=sprintf("%02d:%02d:%02d",$hour,$min,$sec);

$address = inet_ntoa(inet_aton($hostvalue));

if (!defined($address)) {
	print "[$date,$time] Unable to resolve ipadress of $hostvalue, configure dns services properly.\n";
        log_entry("Unable to resolve ipadress of $hostvalue, configure dns services properly.","SystemResolv","$hostvalue");
	return("1");
} else {
	$system_name = gethostbyaddr(inet_aton($address), AF_INET);
}
if (!defined($system_name)) {
	print "[$date,$time] Unable to resolve hostname of $hostvalue from ipadress, configure dns services properly.\n";
        log_entry("Unable to resolve hostname of $hostvalue from ipadress, configure dns services properly.","SystemResolv","$hostvalue");
	return("1");
}
if("$hostvalue" ne "$system_name") {
	print "[$date,$time] Unidentical answers when resolving $hostvalue with adress $address, got=$system_name, configure dns services properly.\n";
        log_entry("Unidentical answers when resolving $hostvalue with adress $address, got=$system_name, configure dns services properly.","SystemResolv","$hostvalue");
	return("1");
} else {
	print "[$date,$time] System $hostvalue resolved, answer ($system_name,$address).\n";
        log_entry("System $hostvalue resolved, answer ($system_name,$address).","SystemResolv","$hostvalue");
	return("0");
}
undef $sth;
$dbh->disconnect or die $!;

if($Enable_forking_mode eq "yes") {
        print "[$date,$time] Closing child. Subfunction back_resolv_connected_system.\n";
        exit;
}

# End of subfunction
}
